<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.spring.calla.Membermapper">


	
	 <!-- 회원정보 등록 -->
	 <insert id="insert">
	 insert into ${member}
	 (${memberId}, ${memberPw}, ${memberName}, ${memberEmail}, ${memberPhone}, ${memberInterest}, ${memberAdress}, ${memberNickname})
	 values (#{memberId}, #{memberPw}, #{memberName}, #{memberEamil}, #{memberPhone}, #{memberInterest}, #{memberAdress}, #{memberNickname})<!-- 물음표 -->
	 </insert>
	 <!-- property의 name과 vo의 변수 이름이 같아야함 -->
	 
	 <!-- 아이디 중복체크 -->
	 <select id="select_by_id" resultType="Integer"> <!-- 셀렉트는 무조건 resulttype이 있어야함 -->
	 	 select count(*) check_cnt from ${member} where ${memberId} = #{memberId}
	 </select>
	 
	 <!-- 닉네임 중복체크 -->
	 <select id="select_by_nickname" resultType="memberVO">
	 	select count(*) from ${member} where ${memberNickname} = #{memberNickname}
	 </select>
	 
	 
	  
</mapper>





